pipeline { 
  
   agent any

   stages {
   
     stage('BUILD') { 
        steps { 
           echo "Welcome to Build Stage"
        }
     }
     
     stage('Test') { 
        steps { 
           echo "testing application..."
        }
      }

         stage("Deploy application") { 
         steps { 
           echo "deploying application..."
         }

     }

     stage("Test Aplication") { 
         steps { 
           echo "Multipiipeline -----application..."
         }

     }
   	}

   }
